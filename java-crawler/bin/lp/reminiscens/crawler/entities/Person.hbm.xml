<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="lp.reminiscens.crawler.entities.Person" table="reminiscens2.Person"
		lazy="true">
		<id column="person_id" name="person_id" type="int">
			<generator class="native" />
		</id>
		<property name="firstName">
			<column name="firstname" />
		</property>
		<property name="fullName">
			<column name="fullname" />
		</property>
		<property name="lastName">
			<column name="lastname" />
		</property>
		<property name="famous">
			<column name="famous" />
		</property>
		<property name="famous_for">
			<column name="famous_for" />
		</property>
		<property name="source">
			<column name="source" />
		</property>
		<property name="source_url">
			<column name="source_url" />
		</property>
		<property name="last_update">
			<column name="last_update" />
		</property>
		<property name="locale">
			<column name="locale" />
		</property>
		<property name="creator_type">
			<column name="creator_type" />
		</property>
	</class>
</hibernate-mapping>
