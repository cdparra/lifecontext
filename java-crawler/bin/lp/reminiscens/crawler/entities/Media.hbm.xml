<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="lp.reminiscens.crawler.entities.Media" table="reminiscens2.Media" lazy="true">
    <id column="media_id" name="media_id" type="int">
      <generator class="native"/>
    </id>
    <property name="media_url">
      <column name="media_url"/>
    </property>
    <property name="media_type">
      <column name="media_type"/>
    </property>
    <property name="caption">
      <column name="caption"/>
    </property>
    <property name="text">
      <column name="text"/>
    </property>
    <property name="source">
      <column name="source"/>
    </property>
    <property name="source_url">
      <column name="source_url"/>
    </property>
    <property name="last_update">
      <column name="last_update"/>
    </property>
    <property name="is_public">
      <column name="is_public"/>
    </property>
    <property name="locale">
      <column name="locale"/>
    </property>
    <many-to-one cascade="all" column="location_id" name="location" unique="true"/>
    <many-to-one cascade="all" column="fuzzy_startdate" name="takenDate" unique="true"/>
  </class>
</hibernate-mapping>
