<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="lp.reminiscens.crawler.entities.Media_Metadata" table="reminiscens2.Media_Metadata" lazy="true">
        <id column="media_metadata_id" name="media_metadata_id" type="int">
            <generator class="native"/>
        </id>
        <property name="title">
            <column name="title"/>
        </property>
        <property name="description">
            <column name="description"/>
        </property>
        <property name="type">
            <column name="type"/>
        </property>
        <property name="source">
            <column name="source"/>
        </property>
        <property name="source_url">
            <column name="source_url"/>
        </property>
        <property name="locale">
            <column name="locale"/>
        </property>
        <property name="resource_url">
            <column name="resource_url"/>
        </property>
        <many-to-one cascade="all" column="fuzzy_releasedate" name="releaseDate" unique="true" lazy="false"/>
    </class>
</hibernate-mapping>
