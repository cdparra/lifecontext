<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="lp.reminiscens.crawler.entities.Fuzzy_Date" table="reminiscensdb.Fuzzy_Date"
		lazy="true">
		<id column="fuzzy_date_id" name="fuzzy_date_id" type="big_integer">
			<generator class="native" />
		</id>
		<property name="exact_date">
			<column name="exact_date" />
		</property>
		<property name="textual_date">
			<column name="textual_date" />
		</property>
		<property name="accuracy">
			<column name="accuracy" />
		</property>
		<property name="locale">
			<column name="locale" />
		</property>
		<property name="decade">
			<column name="decade" />
		</property>
		<property name="year">
			<column name="year" />
		</property>
		<property name="month">
			<column name="month" />
		</property>
		<property name="day">
			<column name="day" />
		</property>
		<property name="day_name">
			<column name="day_name" />
		</property>
		<property name="hour">
			<column name="hour" />
		</property>
		<property name="minute">
			<column name="minute" />
		</property>
		<property name="second">
			<column name="second" />
		</property>
		<property name="day_part">
			<column name="day_part" />
		</property>
		<property name="season">
			<column name="season" />
		</property>
	</class>
</hibernate-mapping>
